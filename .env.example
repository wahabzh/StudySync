# Supabase Configuration
# Get these from: https://app.supabase.com/project/_/settings/api
NEXT_PUBLIC_SUPABASE_URL=your_supabase_url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key

# Tiptap Configuration
# Get these from your Tiptap account settings
NEXT_PUBLIC_TIPTAP_BASE_URL=your_tiptap_base_url
NEXT_PUBLIC_TIPTAP_APP_SECRET=your_tiptap_secret

# Email Service Configuration
# Get this from Resend dashboard
NEXT_PUBLIC_RESEND_API_KEY=your_resend_api_key

# Application Configuration
NEXT_PUBLIC_BASE_URL=http://localhost:3000
# Google Cloud Configuration
# Get these from Google Cloud Console
GOOGLE_CLOUD_PROJECT=your_project_id
GOOGLE_CLOUD_CREDENTIALS={"type": "service_account", ...} # Your JSON credentials
GOOGLE_GENERATIVE_AI_API_KEY=your_ai_api_key
